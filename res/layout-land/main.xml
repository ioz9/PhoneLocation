
<TableLayout android:id="@+id/tableLayout1"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android">
	<TableRow android:id="@+id/tableRow1" android:layout_width="wrap_content"
		android:layout_height="wrap_content">
		<TextView android:layout_height="wrap_content" android:text="@string/time"
			android:layout_width="wrap_content" />
		<TextView android:layout_width="match_parent"
			android:layout_height="wrap_content" android:id="@+id/clock"
			android:text="@string/gps_pending" />
	</TableRow>
	<TableRow android:id="@+id/tableRow1" android:layout_width="wrap_content"
	android:layout_height="wrap_content">
		<TextView android:layout_height="wrap_content" android:text="@string/gpsactive"
			android:layout_width="wrap_content" />
		<TextView android:layout_width="match_parent"
			android:layout_height="wrap_content" android:id="@+id/status"
			android:text="@string/gps_pending" android:background="@drawable/blue"/>
		<TextView android:layout_width="match_parent"
			android:layout_height="wrap_content" android:id="@+id/statusN" android:background="@drawable/dk_grey"
			android:text="@string/gps_pending" />
	</TableRow>
	<TableRow android:id="@+id/tableRow2" android:layout_width="wrap_content"
		android:layout_height="wrap_content">
		<TextView android:layout_height="wrap_content" android:text="@string/gps_long_lbl"
			android:layout_width="wrap_content" />
		<TextView android:layout_width="match_parent"
			android:layout_height="wrap_content" android:id="@+id/longitude"
			android:text="@string/gps_pending" android:background="@drawable/blue"/>
		<TextView android:layout_width="match_parent"
			android:layout_height="wrap_content" android:id="@+id/longitudeN" android:background="@drawable/dk_grey"
			android:text="@string/gps_pending" />
</TableRow>
		<TableRow android:id="@+id/tableRow2" android:layout_width="wrap_content"
			android:layout_height="wrap_content">
			<TextView android:layout_height="wrap_content" android:text="@string/gps_lat_lbl"
				android:layout_width="wrap_content" />
			<TextView android:layout_width="match_parent"
				android:layout_height="wrap_content" android:id="@+id/latitude"  android:background="@drawable/blue"
				android:text="@string/gps_pending" />
			<TextView android:layout_width="match_parent"
				android:layout_height="wrap_content" android:id="@+id/latitudeN" android:background="@drawable/dk_grey"
				android:text="@string/gps_pending" />

		</TableRow>
		<TableRow android:id="@+id/tableRow3" android:layout_width="wrap_content"
			android:layout_height="wrap_content">
			<TextView android:layout_height="wrap_content" android:text="@string/gps_alt_lbl"
				android:layout_width="wrap_content" />
			<TextView android:layout_width="match_parent"
				android:layout_height="wrap_content" android:id="@+id/altitude" android:background="@drawable/blue"
				android:text="@string/gps_pending" />
			<TextView android:layout_width="match_parent"
				android:layout_height="wrap_content" android:id="@+id/altitudeN" android:background="@drawable/dk_grey"
				android:text="@string/gps_pending" />
		</TableRow>
		<TableRow android:id="@+id/tableRow3" android:layout_width="wrap_content"
			android:layout_height="wrap_content">
			<TextView android:layout_height="wrap_content" android:text="@string/gps_accuracy_lbl"
				android:layout_width="wrap_content" />
			<TextView android:layout_width="match_parent"
				android:layout_height="wrap_content" android:id="@+id/accuracy" android:background="@drawable/blue"
				android:text="@string/gps_pending" />
			<TextView android:layout_width="match_parent"
				android:layout_height="wrap_content" android:id="@+id/accuracyN" android:background="@drawable/dk_grey"
				android:text="@string/gps_pending" />
		</TableRow>
		<TableRow android:id="@+id/tableRow4" android:layout_width="wrap_content"
			android:layout_height="wrap_content">
			<TextView android:layout_height="wrap_content" android:text="@string/gps_speed_lbl" 
				android:layout_width="wrap_content" />
			<TextView android:layout_width="match_parent"
				android:layout_height="wrap_content" android:id="@+id/speed" android:background="@drawable/blue"
				android:text="@string/gps_pending" />
			<TextView android:layout_width="match_parent"
				android:layout_height="wrap_content" android:id="@+id/speedN"  android:background="@drawable/dk_grey"
				android:text="@string/gps_pending" />
		</TableRow>
		<TableRow android:id="@+id/tableRow4" android:layout_width="wrap_content"
			android:layout_height="wrap_content">
			<TextView android:layout_height="wrap_content" android:text="@string/gps_age_lbl" 
				android:layout_width="wrap_content" />
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:id="@+id/age" android:background="@drawable/blue"
				android:text="@string/gps_pending" />
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:id="@+id/ageN" android:background="@drawable/dk_grey"
				android:text="@string/gps_pending" />
		</TableRow>
		<TableRow android:id="@+id/tableRow5" android:layout_width="wrap_content"
			android:layout_height="wrap_content">
			<TextView android:layout_height="wrap_content" android:text="@string/gps_provider_lbl"
				android:layout_width="wrap_content" />
			<TextView android:layout_width="match_parent"
				android:layout_height="wrap_content" android:id="@+id/provider" android:background="@drawable/blue"
				android:text="@string/gps_pending" />
			<TextView android:layout_width="match_parent"
				android:layout_height="wrap_content" android:id="@+id/providerN" android:background="@drawable/dk_grey"
				android:text="@string/gps_pending" />
		</TableRow>
		<TableRow android:id="@+id/tableRow6" android:layout_width="wrap_content"
			android:layout_height="wrap_content">
			<TextView android:layout_height="wrap_content" android:text="@string/distancebwtn"
				android:layout_width="wrap_content" />
			<TextView android:layout_width="match_parent"
				android:layout_height="wrap_content" android:id="@+id/distance"
				android:text="@string/gps_pending" />
		</TableRow>
</TableLayout>
